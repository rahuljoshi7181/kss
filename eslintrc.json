{
    "root": true,
    "parserOptions": {
         "ecmaVersion": 12
    },
    "extends": ["eslint:recommended", "prettier","plugin:node/recommended"],
    "env": { 
         "es2021": true,
         "node": true
    },
    "plugins": [
     "unused-imports"
     ],
    "rules": {
         "no-console": "error",
         "unused-imports/no-unused-vars": "error"
    }
}